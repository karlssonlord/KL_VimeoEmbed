{
    "name": "karlssonlord/KL_VimeoEmbed",
    "description": "Embed Vimeo videos",
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "magento-hackathon/magento-composer-installer": "2.1.1",
        "magento/core": "1.9.1.0"
    },
    "authors": [
        {
            "name": "David Wickstr√∂m",
            "email": "davidwickstrom@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "composer",
          "url": "http://packages.firegento.com"
        }
    ],
    "extra": {
        "auto-append-gitignore": true,
        "magento-root-dir": "src",
        "magento-deploystrategy": "symlink",
        "magento-force": true
    },
    "autoload": {
        "psr-0": {
            "": [
                "src/app",
                "src/app/code/local",
                "src/app/code/community",
                "src/app/code/core",
                "src/lib"
            ]
        }
    },
    "minimum-stability": "dev"
}
